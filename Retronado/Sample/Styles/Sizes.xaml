<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>

<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <x:Double x:Key="SpacingSmall">5</x:Double>
    <x:Double x:Key="SpacingMedium">10</x:Double>
    <x:Double x:Key="SpacingLarge">15</x:Double>

    <Thickness x:Key="ThicknessSmall">5</Thickness>
    <Thickness x:Key="ThicknessMedium">10</Thickness>
    <Thickness x:Key="ThicknessLarge">15</Thickness>

    <OnPlatform x:Key="GridLengthNavigationBarHeight" x:TypeArguments="GridLength">
        <OnPlatform.Platforms>
            <On Platform="Android" Value="56" />
            <On Platform="iOS" Value="44" />
            <On Platform="UWP" Value="50" />
        </OnPlatform.Platforms>
    </OnPlatform>

    <x:Double x:Key="ToolBarHeight">70</x:Double>

</ResourceDictionary>
